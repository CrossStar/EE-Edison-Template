@thesis{ZhangJiaWeiSiZuJiQiRenYongZhouXiangCiTongYongCiDianJiSheJiYuFenXi2024,
  type = {硕士学位论文},
  title = {四足机器人用轴向磁通永磁电机设计与分析},
  author = {{张嘉伟}},
  namea = {{刘建勇} and {马丽梅}},
  nameatype = {collaborator},
  date = {2024},
  journaltitle = {工程科技Ⅱ辑;信息科技},
  institution = {北京石油化工学院},
  doi = {10.27849/d.cnki.gshyj.2023.000114},
  url = {https://kns.cnki.net/KCMS/detail/detail.aspx?dbcode=CMFD&dbname=CMFDTEMP&filename=1023739739.nh},
  urldate = {2024-08-20},
  abstract = {火灾救援现场情况复杂多变,存在高温、浓烟、坍塌等潜在危险,消防人员无法及时对周围环境及受伤人员位置进行判断,救援效率低下。四足机器人凭借其灵活的行走能力以及较强的负载能力成为救援领域的最优选择。四足机器人的机动性及负载能力直接受电驱关节的影响。电机作为四足机器人电驱关节的核心部件,其所具备的转矩密度性能是整个四足机器人的机动性和负载能力的直接来源。通过研究国内外四足机器人电驱关节用电机,发现传统的径向磁通永磁电机普遍存在转矩密度低、体积大等问题,限制了四足机器人的性能发挥。针对上述问题,设计了一种无极靴结构的新型轴向磁通永磁电机。本文主要研究内容包括以下几个方面:首先,基于四足机器人电驱关节对电机的性能需求,综合分析了永磁电机各个方案的优劣,设计了一种高转矩密度的无极靴结构的新型轴向磁通永磁电机。对轴向磁通永磁电机在多工况的条件下进行仿真,结果表明,该电机满足电机性能需求,相比传统电机具有更高的转矩密度,为四足机器人的性能提升提供了新的方案。然后,基于电机气隙磁场沿轴向分布的特性及电机的无极靴结构,对电机气隙磁场进行分析,推导出电机电磁转矩解析模型。在电磁转矩解析模型的基础上,借助Genetic Algorithm优化器对电机的转矩密度进行优化,并基于新的电机参数建立电机模型,随后利用有限元法对优化前电机模型及优化后电机模型进行仿真验证,结果表明,优化后的电机转矩密度提升了20\%,达到了5.34 N.m/kg。最后,对电驱关节进行制造与测试。测试结果表明,搭载了轴向磁通永磁电机的电驱关节额定输出转矩超过38 N.m,满足电驱关节设计需求,且输出转矩与输入电流之间具有良好的线性度。无极靴结构的新型轴向磁通永磁电机为四足机器人强机动性及高负载能力需求提供了新的思路和技术支撑。},
